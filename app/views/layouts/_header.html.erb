<header class="header navbar nabvar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to 'ダミーシステム(仮)', root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar navbar-right" id="nav">
        <% if user_signed_in? %>
          <li><%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class: "nav-link" %></li>
        <% else %>
          <li><%= link_to '新規登録', new_user_registration_path, class: "nav-link" %></li>
          <li><%= link_to 'ログイン', new_user_session_path, class: "nav-link" %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>